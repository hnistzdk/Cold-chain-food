<?xml version="1.0" encoding="UTF8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zdk.mapper.UserMapper">

    <select id="queryFoodById" resultType="food">
        select * from food.tb_food where id = #{id};
    </select>

    <select id="queryFoodList" resultType="food">
        select * from food.tb_food;
    </select>
</mapper>